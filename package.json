{
  "name": "my-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "analyz": "vue-cli-service build --mode analyz",
    "dev": "vue-cli-service serve",
    "fix:css": "stylelint --fix src/**/*.{vue,html,css,scss,sass,less}",
    "lint:css": "stylelint src/**/*.{vue,html,css,scss,sass.less}",
    "new": "plop",
    "report": "vue-cli-service build --report",
    "svg": "vsvg -s ./src/icons/svg -t ./src/icons/components --ext js --es6"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "element-ui": "^2.12.0",
    "jquery": "^3.4.1",
    "js-cookie": "*",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vue-svgicon": "^3.2.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.4",
    "@vue/cli-plugin-e2e-cypress": "^4.4.4",
    "@vue/cli-plugin-eslint": "^4.4.4",
    "@vue/cli-plugin-pwa": "^4.4.4",
    "@vue/cli-plugin-unit-jest": "^4.4.4",
    "@vue/cli-service": "^4.4.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.4.2",
    "plop": "^2.3.0",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.13.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "stylelint-webpack-plugin": "^1.1.2",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.2",
    "terser-webpack-plugin": "^2.3.1",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
